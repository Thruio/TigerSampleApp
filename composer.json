{
  "name": "thru.io/starter-app",
  "description": "Skeleton application using thru.io/tiger-kit",
  "license": "GPL V2",
  "minimum-stability": "dev",
  "php": ">=5.5.0",
  "authors": [
    {
      "name": "Matthew Baggett",
      "email": "matthew@baggett.me"
    }
  ],
  "repositories": [
    {
      "url": "https://github.com/Thruio/TigerKit.git",
      "type": "git"
    },
    {
      "url": "https://github.com/Thruio/ActiveRecord.git",
      "type": "git"
    },
    {
      "url": "https://github.com/Thruio/ActiveRecordMigrations.git",
      "type": "git"
    },
    {
      "url": "https://github.com/Thruio/Session.git",
      "type": "git"
    },
    {
      "url": "https://github.com/Thruio/Translation.git",
      "type": "git"
    }
  ],
  "require": {
    "thru.io/tiger-kit": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "SampleApp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SampleApp\\Test\\": "tests/"
    }
  }
}
